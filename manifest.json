{
  "manifest_version": 2,
  "name": "graytabby",
  "version": "1.0.0",
  "icons": {
    "128": "assets/img/icon128.png",
    "48": "assets/img/icon48.png",
    "16": "assets/img/icon16.png"
  },
  "browser_action": {
    "default_icon": "assets/img/icon16.png",
    "default_title": "Archive into GrayTabby"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}
